    "query": {
        "from": {
            "table": "Kopokopo\\Transactions\\Code\\Tables\\Transactions",
            "alias": "kt",
            "name": "#__kopokopo_transactions"
        },
        "select": [
            "kt.id",
            "kt.transaction_reference",
            "kt.name",
            "kt.sender_phone",
            "kt.used_by",
            "uu.name AS used_by_name",
            "uu.username AS used_by_username",
            "kt.used",
            "kt.service_name",
            "kt.business_number",
            "kt.internal_transaction_id",
            "kt.transaction_timestamp",
            "kt.transaction_type",
            "kt.account_number",
            "kt.first_name",
            "kt.middle_name",
            "kt.last_name",
            "kt.amount",
            "kt.currency",
            "kt.created_by",
            "uu_1081.name AS created_by_name",
            "kt.date_created",
            "kt.modified_by",
            "uu_8559.name AS modified_by_name",
            "kt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "kt.used_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1081",
                "name": "#__users_users",
                "on": "kt.created_by = uu_1081.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8559",
                "name": "#__users_users",
                "on": "kt.modified_by = uu_8559.id"
            }
        ]
    }
